{
  "name": "watch_me_api_ts",
  "version": "1.0.0",
  "description": "WatchMe API is an API to interact with the CNN 'Watch Me', connecting the Machine Learning end of the program with a user UI in React.",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:create:dev": "createdbjs watchme_dev --user=postgres --password=postgres --host=127.0.0.1",
    "db": "tsx ./node_modules/knex/bin/cli.js",
    "db:migrate:dev": "npm run db migrate:latest",
    "db:seed:dev": "npm run db seed:run",
    "db:rollback:dev": "npm run db migrate:rollback"
  },
  "bugs": {
    "url": "https://github.com/beatrizamante/watchme_api_backend/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beatrizamante/watchme_api_backend.git"
  },
  "homepage": "https://github.com/beatrizamante/watchme_api_backend#readme",
  "keywords": [],
