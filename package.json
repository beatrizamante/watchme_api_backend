  "license": "ISC",
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.1",
    "fastify": "^5.4.0",
    "knex": "^3.1.0",
    "objection": "^3.1.5",
    "pg": "^8.16.3",
    "pgtools": "^1.0.1",
    "pino": "^9.9.0",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
